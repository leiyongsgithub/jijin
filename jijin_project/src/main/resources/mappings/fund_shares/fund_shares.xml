<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.spring.boot.jijin.fund_shares.dao.FundSharesDao">
	
	<insert id="save" parameterType="com.test.spring.boot.jijin.fund_shares.entity.FundSharesEntity">
        insert into fund_shares
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="fundId != null">fund_id,</if>
            <if test="sharesId != null">shares_id,</if>
            <if test="holdingRatio != null">holding_ratio</if>
        </trim>
        <trim prefix="values(" suffix=")" suffixOverrides=",">
            <if test="fundId != null">#{fundId},</if>
            <if test="sharesId != null">#{sharesId},</if>
            <if test="holdingRatio != null">#{holdingRatio}</if>
        </trim>
    </insert>
</mapper>