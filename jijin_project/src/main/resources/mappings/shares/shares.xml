<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.spring.boot.jijin.shares.dao.SharesDao">
	
	<insert id="save" parameterType="com.test.spring.boot.jijin.shares.entity.SharesEntity">
        insert into shares
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="sharesId != null">shares_id,</if>
            <if test="sharesName != null">shares_name</if>
        </trim>
        <trim prefix="values(" suffix=")" suffixOverrides=",">
            <if test="sharesId != null">#{sharesId},</if>
            <if test="sharesName != null">#{sharesName}</if>
        </trim>
    </insert>
</mapper>